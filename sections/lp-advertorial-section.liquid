    </a>
  </div>

  <script>
    $(document).ready(function(){
      $('.faq_question').click(function() {
        $(this).closest('.faq_item').find('.faq_answer').slideToggle();
        $(this).closest('.faq_item').toggleClass('active');
      });
  
      $('.faq_see_more').click(function() {
        $('.faq_item').removeClass('faq_hidden');
        $(this).hide();
      });
  
      $('.upsell_usp_atc_btn, .right_upsell_usp_atc_btn').click(function() {
        const id = $(this).data('id');
        add_to_cart(id, 1, undefined);
      });
  
      const slideToUpsellUspSection = () => {
        $('html, body').animate({
          scrollTop: $(`#upsell_usp`).offset().top
        }, 500);
      };
  
      setTimeout(() => {
        // Reviews to review_container
        $('{{ _section }} .review_container').append($('.shopify-section:has(#looxReviews)'));
        // Review Slider to review_slider_container
        $('{{ _section }} .review_slider_container').append($('.shopify-section:has(.swiper-reviews)'));
        // Ingredients to ingredients_container 
        $('{{ _section }} .ingredients_container').append($('.shopify-section:has(.ingredients)'));
      }, 1000);
    })
  </script>
{%- endcapture -%}

