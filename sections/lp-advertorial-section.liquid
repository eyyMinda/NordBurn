{% assign _ss = section.settings %}
{% assign _section = section.id | split: '__' | last | slice: -4, 4 | prepend: '#lp_advertorial_' %}

{%- capture section_content -%}
  <style>
    {{ _section }} .product_section_holder {
      --main-text-color: {{ _ss.main_text_color | default: '#000000' }};
      --accent-color: {{ _ss.accent_color | default: '#000000' }};
      max-width: 1600px;
      position: relative;
      padding: 24px 16px;
      margin-inline: auto;
    }
    /* =================================== General Layout ================= */
    {{ _section }} :is(.text_container, .subtitle_container):first-of-type {
      margin-top: 0;
    }
    {{ _section }} :is(.text_container, .subtitle_container):last-of-type {
      margin-bottom: 0;
    }
    /* =================================== TrustPilot Review ================= */
    {{ _section }} .trustpilot_review {
      margin: 30px auto 8px;
      padding: 5px 16px;
      width: 100%;
      text-align: center;
    }
    {{ _section }} .trustpilot_review img {
      max-width: 500px;
      width: 100%;
    }
    /* =================================== Title ================= */
    {{ _section }} .title {
      color: var(--main-text-color);
      text-align: center;
      font-family: "Lato";
      font-weight: 700;
      font-size: 28px;
      line-height: 1.2;
      margin: 0 14px 10px;
    }
    /* =================================== Date / Reading Time ================= */
    {{ _section }} .date_reading_time {
      color: #BFBFBF;
      text-align: center;
      font-family: "Lato";
      font-style: normal;
      font-weight: 400;
      font-size: 16px;
      line-height: 0.8;
      margin: 0 14px 26px;
    }
    
    /* =================================== Image ================= */
    {{ _section }} .image_container {
      margin: 26px 14px;
      width: calc(100% - 28px);
      border-radius: 5px;
      overflow: hidden;
      transform: translateZ(0);
    }
    {{ _section }} .image_container img {
      width: 100%;
    }
    /* =================================== SubTitle ================= */
    {{ _section }} .subtitle_container {
      margin-block: 32px 16px;
    }
    {{ _section }} .subtitle_container p {
      margin: 0;
      color: black;
      font-family: "Lato";
      font-style: normal;
      font-weight: 700;
      font-size: 24px;
      line-height: 1.1;
    }
    {{ _section }} .subtitle_container p strong {
      color: var(--accent-color);
    }
    /* =================================== Text ================= */
    {{ _section }} .text_container {
      margin-block: 20px;
    }
    {{ _section }} .text_container p {
      color: black;
      font-family: "Lato";
      font-style: normal;
      font-size: 16px;
      font-weight: 400;
      line-height: 1.3;
      margin: 0;
    }
    {{ _section }} .text_container p strong {
      font-weight: 700;
      color: var(--accent-color);
    }
    /* =================================== Quote ================= */
    {{ _section }} .quote_container {
      margin-block: 20px;
      border-radius: 5px;
      background: var(--main-text-color);
      padding: 15px;
      display: flex;
      box-shadow: 0px 4px 4px 0px #00000040;
    }
    {{ _section }} .quote_container::before {
      content: ' ';
      display: block;
      margin-right: 12px;
      width: 4px;
      min-width: 4px;
      height: auto;
      border-radius: 99px;
      background: #E0FDD6;
    }
    {{ _section }} .quote_holder p {
      margin: 0;
      color: #FFF;
      font-family: "Lato";
      font-size: 18px;
      font-style: italic;
      font-weight: 500;
      line-height: 1.2;
    }
    {{ _section }} .quote_holder p strong {
      color: #E0FDD6;
    }
    /* =================================== Warning ================= */
    {{ _section }} .warning_container {
      margin-block: 20px;
      border-radius: 5px;
      background: var(--main-text-color);
      padding: 18px 17px;
      display: flex;
    }
    {{ _section }} .warning_container::before {
      content: ' ';
      display: block;
      margin-right: 12px;
      width: 4px;
      min-width: 4px;
      height: auto;
      border-radius: 99px;
      background: #FE4643;
    }
    {{ _section }} .warning_title {
      color: #FE4643;
      font-family: "Lato";
      font-size: 19px;
      font-style: normal;
      font-weight: 700;
      line-height: 1.2;
      margin-bottom: 8px;
    }
    {{ _section }} .warning_text p {
      margin: 0 0 16px 0;
      color: #FFF;
      font-family: "Lato";
      font-size: 14px;
      font-style: normal;
      font-weight: 400;
      line-height: 1.3;
    }
    {{ _section }} .warning_text p:last-child {
      margin-bottom: 0;
    }
    /* =================================== Half Decrease ================= */
    {{ _section }} .half_decrease_container {
      margin-block: 20px;
      display: flex;
      align-items: center;
    }
    {{ _section }} .half_decrease_container .mobile_image {
      display: block;
      margin-right: 14px;
      width: 87px;
      min-width: 87px;
      height: 61px;
    }
    {{ _section }} .half_decrease_container .desktop_image {
      display: none;
    }
    {{ _section }} .half_decrease_container p {
      margin: 0;
      color: var(--main-text-color);
      font-family: "Lato";
      font-size: 24px;
      font-style: normal;
      font-weight: 700;
      line-height: 1.2;
    }
    /* =================================== Text over Image ================= */
    {{ _section }} .text_over_image_container {
      margin-block: 20px;
      height: 350px;
      position: relative;
      border-radius: 5px;
      overflow: hidden;
      transform: translateZ(0);
    }
    {{ _section }} .text_over_image_container img {
      display: block;
      width: 100%;
      height: 100%;
      object-fit: cover;
    }
    {{ _section }} .text_over_image_container .text_container {
      position: absolute;
      top: 0;
      left: 0;
      width: calc(100% - 40px);
      margin: 32px 20px;
    }
    {{ _section }} .text_over_image_container .main_text {
      margin-bottom: 16px;
    }
    {{ _section }} .text_over_image_container .main_text p {
      margin: 0;
      color: #37523A;
      text-align: center;
      font-family: "Lato";
      font-size: 24px;
      font-style: normal;
      font-weight: 700;
      line-height: 1.2;
    }
    {{ _section }} .text_over_image_container .main_text p strong {
      color: var(--accent-color);
    }
    {{ _section }} .text_over_image_container .normal_text {
      color: #37523A;
      text-align: center;
      font-family: "Lato";
      font-size: 14px;
      font-style: normal;
      font-weight: 700;
      line-height: 1.2;
      text-transform: uppercase;
    }
    /* =================================== Simple Upsell ================= */
    {{ _section }} .simple_upsell_container {
      margin-block: 20px;
      padding: 15px;
      border-radius: 5px;
      background: #FBFBF7;
    }
    {{ _section }} .simple_upsell_container img {
      display: block;
      margin-bottom: 15px;
      width: 100%;  
    }
    {{ _section }} .simple_upsell_button {
      display: block;
      text-decoration: none;
      border-radius: 99px;
      background: var(--main-text-color);
      text-align: center;
      padding-top: 18px;
      padding-bottom: 18px;
      color: #E0FDD6;
      text-align: center;
      font-family: "Lato";
      font-size: 18px;
      font-style: normal;
      font-weight: 700;
      line-height: 1.2;
      cursor: pointer;
    }
    /* =================================== Col Blocks (Column) ================= */
    {{ _section }} .col-blocks {
      border-radius: 10px;
      display: flex;
      flex-direction: column;
      align-items: center;
      gap: 24px;
      padding: 16px;
      margin-block: 20px;
    }
    {{ _section }} .col-item {
      width: 100%;
      border-radius: 10px;
      display: flex;
      flex-direction: column;
      gap: 16px;
      font-family: "Lato";
      padding: 16px;
    }
    {{ _section }} .col-item__title {
      font-weight: 700;
      font-size: 22px;
      line-height: 1.4;
      text-decoration: underline;
    }
    {{ _section }} .col-item__text p {
      font-weight: 400;
      font-size: 18px;
      line-height: 1.4;
      margin: 0;
    }
    /* =================================== Flex Blocks (ROW) ================= */
    {{ _section }} .flex-blocks {
      border-radius: 10px;
      display: flex;
      flex-direction: column;
      align-items: center;
      gap: 24px;
      margin-block: 20px;
    }
    {{ _section }} .flex-item {
      max-width: 400px;
      width: 100%;
      border-radius: 10px;
      display: flex;
      flex-direction: column;
      align-items: center;
      gap: 16px;
      font-family: "Lato";
      text-align: center;
      padding: 16px 8px;
    }
    {{ _section }} .flex-item > * {
      max-width: 320px;
      width: 100%;
    }
    {{ _section }} .flex-item__num {
      background-color: #FBFBF7;
      border-radius: 50%;
      color: #283618;
      width: 60px;
      height: 60px;
      display: flex;
      justify-content: center;
      align-items: center;
      font-family: "Lato";
      font-size: 38px;
      font-weight: 700;
      line-height: 1;
      text-align: center;
    }
    {{ _section }} .flex-item__title {
      font-weight: 700;
      font-size: 24px;
      line-height: 1.4;
    }
    {{ _section }} .flex-item__text {
      font-weight: 400;
      font-size: 18px;
      line-height: 1.4;
    }
    /* =================================== Long Button ================= */
    {{ _section }} .long-button {
      --accent-color: #1E9E48;
      --foreground-color: #FFFFFF;
      display: block;
      border-radius: 10px;
      border: 2px solid var(--accent-color);
      background-color: var(--accent-color);
      color: var(--foreground-color);
      appearance: none;
      font-family: "Lato";
      font-weight: 700;
      font-size: 18px;
      line-height: 1.2;
      text-align: center;
      text-decoration: underline;
      padding: 16px 8px;
      margin-block: 24px;
      transition: all 200ms ease-in-out;
    }
    {{ _section }} .long-button:hover {
      background-color: var(--foreground-color);
      color: var(--accent-color);
      opacity: 1;
    }
    /* =================================== USPs ================= */
    {{ _section }} .usp_container {
      margin-block: 20px;
    }
    {{ _section }} .usp_title {
      margin-bottom: 24px;
      color: var(--main-text-color);
      font-family: "Lato";
      font-size: 24px;
      font-style: normal;
      font-weight: 700;
      line-height: 1.2;
    }
    {{ _section }} .usp_container .usp_item {
      margin-bottom: 21px;
      padding-bottom: 18px;
      border-bottom: 1px solid #D1D5DB;
    }
    {{ _section }} .usp_container .heading_container {
      display: flex;
      align-items: center;
      margin-bottom: 12px;
    }
    {{ _section }} .usp_container .heading_container:has(.normal_container) {
      margin: 0;
    }
    {{ _section }} .usp_container .heading_container img {
      display: block;
      width: 35px;
      min-width: 35px;
      height: 35px;
      margin-right: 13px;
    }
    {{ _section }} .usp_container .heading_container .usp_item_inline_heading {
      display: flex;
      gap: 12px;
    }
    {{ _section }} .usp_container .heading_container .usp_item_inline_heading.heading-column-mb {
      flex-direction: column;
    }
    {{ _section }} .usp_container .heading_container p {
      color: var(--main-text-color);
      font-family: "Lato";
      font-weight: 700;
      font-style: normal;
      font-size: 18px;
      line-height: 1.2;
      margin-block: 0;
    }
    {{ _section }} .usp_container .normal_container {
      color: var(--accent-color);
      font-family: "Lato";
      font-style: normal;
      font-weight: 400;
      font-size: 16px;
      line-height: 1.3;
    }
    /* =================================== Upsell USPs ================= */
    {{ _section }} .upsell_usp_container {
      margin-block: 40px;
    }
    {{ _section }} .upsell_usp_subtitle {
      color: var(--main-text-color);
      text-align: center;
      font-family: "Lato";
      font-size: 20px;
      font-style: normal;
      font-weight: 700;
      line-height: 1.2;
      margin-bottom: 12px;
    }
    {{ _section }} .upsell_usp_title {
      color: var(--accent-color);
      text-align: center;
      font-family: "Lato";
      font-size: 26px;
      font-style: normal;
      font-weight: 700;
      line-height: 1.2;
      margin-bottom: 21px;
      object-fit: cover;
    }
    {{ _section }} .upsell_usp_image {
      display: block;
      width: 100%;
      border-radius: 5px;
      height: 350px;
      overflow: hidden;
      object-fit: cover;
      margin-bottom: 20px;
      transform: translateZ(0);
    }
    {{ _section }} .upsell_usp_items {
      display: grid;
      grid-template-columns: repeat(2, 1fr);
      gap: 15px 6px;
      margin-bottom: 21px;
    }
    {{ _section }} .upsell_usp_item {
      display: flex;
      align-items: center;
    }
    {{ _section }} .upsell_usp_item svg {
      display: block;
      width: 21px;
      min-width: 21px;
      height: 21px;
      margin-right: 7px;
    }
    {{ _section }} .upsell_usp_item svg path {
      fill: #37523A;
    }
    {{ _section }} .upsell_usp_item p {
      margin: 0;
      color: #000;
      font-family: "Lato";
      font-size: 14px;
      font-style: normal;
      font-weight: 500;
      line-height: 1.2;
    }
    {{ _section }} .upsell_usp_quantity {
      margin: 0 14px 15px;
    }
    {{ _section }} .upsell_usp_quantity_progress {
      margin-bottom: 8px;
      position: relative;
      width: 100%;
      height: 6px;
      border-radius: 99px;
      border: 1px solid var(--main-text-color);
      background: rgba(47, 19, 19, 0.00);
    }
    {{ _section }} .upsell_usp_quantity_progress .progress_container {
      display: block;
      border-radius: 99px;
      position: absolute;
      height: 6px;
      background: #FE4643;
      top: -1px;
      left: -1px;
    }
    {{ _section }} .upsell_usp_text {
      color: #000;
      text-align: center;
      font-family: "Lato";
      font-size: 14px;
      font-style: normal;
      font-weight: 500;
      line-height: 1.2;
    }
    {{ _section }} .upsell_usp_text span {
      color: #FE4643;
      font-weight: 700;
    }
    {{ _section }} .upsell_usp_atc_btn_wrapper {
      margin-bottom: 8px;
      width: 100%;
      height: 55px;
      border-radius: 30px;
      background: #23D160;
      position: relative;
      overflow: hidden;
      padding: 5px;
      transform: translateZ(0);
    }
    {{ _section }} .upsell_usp_atc_btn_wrapper::before {
      content: '';
      display: block;
      background: conic-gradient(#23D160, #23D160, #23D160, #23D160, #FAF6F0, #fff);
      width: 110%;
      padding-bottom: 216%;
      position: absolute;
      left: 50%;
      top: 50%;
      transform: translate(-50%, -50%);
      border-radius: 100%;
      z-index: 2;
      animation: button_border_spin 2.5s linear infinite;
    }
    {{ _section }} .upsell_usp_atc_btn {
      position: absolute;
      width: calc(100% - 10px);
      height: 45px;
      border-radius: 30px;
      background: #23D160;
      display: flex;
      align-items: center;
      justify-content: center;
      cursor: pointer;
      z-index: 3;
      color: #FFF;
      text-align: center;
      font-family: "Lato";
      font-size: 23px;
      font-style: normal;
      font-weight: 700;
      line-height: 1.2;
    }
    {{ _section }} .upsell_usp_atc_texts {
      display: flex;
      margin: 0 auto 15px;
      width: fit-content;
      height: 18px;
    }
    {{ _section }} .upsell_usp_atc_text {
      display: block;
      color: var(--main-text-color);
      font-family: "Lato";
      font-size: 14px;
      font-style: normal;
      font-weight: 500;
      line-height: 1.2;
      padding: 0 12px;
      border-right: 1px solid var(--main-text-color);
    }
    {{ _section }} .upsell_usp_atc_text:first-child {
      padding-left: 0;
    }
    {{ _section }} .upsell_usp_atc_text:last-child {
      padding-right: 0;
      border-right: none;
    }
    {{ _section }} .upsell_usp_payment_container {
      display: flex;
      margin: 0 auto;
      width: fit-content;
      height: 24px;
    }
    {{ _section }} .upsell_usp_payment_item {
      display: block;
      width: 36px;
      height: 24px;
      border-radius: 2px;
      margin-right: 8px;
    }
    {{ _section }} .upsell_usp_payment_item:last-child {
      margin-right: 0;
    }
    {{ _section }} .upsell_usp_payment_item img {
      display: block;
      width: 100%;
      height: 100%;
    }
    /* =================================== Review ================= */
    {{ _section }} .review_container {
      background: #FBFBF7;
      margin-block: 38px;
    }
    {{ _section }} .review_title {
      margin: 0 auto 12px;
      padding: 0 55px;
      color: var(--main-text-color);
      text-align: center;
      font-family: "Lato";
      font-size: 24px;
      font-style: normal;
      font-weight: 700;
      line-height: 1.2;
    }
    /* =================================== Upsell Medium ================= */
    {{ _section }} .upsell_medium_container {
      margin-block: 40px;
    }
    {{ _section }} .upsell_medium_subtitle {
      color: var(--main-text-color);
      font-family: "Lato";
      font-size: 20px;
      font-style: normal;
      font-weight: 700;
      line-height: 1.2;
      margin-bottom: 12px;
    }
    {{ _section }} .upsell_medium_title {
      color: var(--accent-color);
      font-family: "Lato";
      font-size: 26px;
      font-style: normal;
      font-weight: 700;
      line-height: 1.2;
      margin-bottom: 21px;
    }
    {{ _section }} .upsell_medium_description {
      color: black;
      font-family: "Lato";
      font-style: normal;
      font-weight: 400;
      font-size: 18px;
      line-height: 1.2;
      text-align: center;
      margin-bottom: 12px;
    }
    {{ _section }} .upsell_medium_content {
      border-radius: 5px;
      background: #0C2016;
      padding: 21px 15px;
    }
    {{ _section }} .upsell_medium_content .desktop_container {
      display: none;
    }
    {{ _section }} .upsell_medium_image {
      display: block;
      width: 100%;
      margin-bottom: 16px;
    }
    {{ _section }} .upsell_medium_quantity {
      margin-bottom: 8px;
    }
    {{ _section }} .upsell_medium_quantity_progress {
      display: block;
      height: 6px;
      position: relative;
      border-radius: 99px;
      border: 1px solid #FFF;
      background: rgba(47, 19, 19, 0.00);
      margin-bottom: 8px;
    }
    {{ _section }} .upsell_medium_quantity_progress .progress_container {
      display: block;
      border-radius: 99px;
      position: absolute;
      height: 6px;
      background: #FE4643;
      top: -1px;
      left: -1px;
    }
    {{ _section }} .upsell_medium_quantity .upsell_medium_text {
      color: #FFF;
      text-align: center;
      font-family: "Lato";
      font-size: 14px;
      font-style: normal;
      font-weight: 500;
      line-height: 1.2;
    }
    {{ _section }} .upsell_medium_quantity .upsell_medium_text span {
      color: #FE4643;
      font-weight: 700;
    }
    {{ _section }} .upsell_medium_atc_btn {
      display: block;
      width: 100%;
      max-width: 500px;
      background: var(--accent-color);
      border-radius: 99px;
      padding-top: 18px;
      padding-bottom: 18px;
      text-decoration: none;
      color: #FFF;
      text-align: center;
      font-family: "Lato";
      font-size: 19px;
      font-style: normal;
      font-weight: 700;
      line-height: 1.2;
      cursor: pointer;
      margin: 0 auto 8px;
    }
    {{ _section }} .upsell_medium_atc_texts {
      display: flex;
      margin: 0 auto 15px;
      width: fit-content;
      height: 18px;
    }
    {{ _section }} .upsell_medium_atc_text {
      display: block;
      color: #fff;
      font-family: "Lato";
      font-size: 14px;
      font-style: normal;
      font-weight: 500;
      line-height: 1.2;
      padding: 0 12px;
      border-right: 1px solid #fff;
    }
    {{ _section }} .upsell_medium_atc_text:first-child {
      padding-left: 0;
    }
    {{ _section }} .upsell_medium_atc_text:last-child {
      padding-right: 0;
      border-right: none;
    }
    {{ _section }} .upsell_medium_payment_container {
      display: flex;
      margin: 0 auto;
      width: fit-content;
      height: 24px;
    }
    {{ _section }} .upsell_medium_payment_item {
      display: block;
      width: 36px;
      height: 24px;
      border-radius: 2px;
      margin-right: 8px;
    }
    {{ _section }} .upsell_medium_payment_item:last-child {
      margin-right: 0;
    }
    {{ _section }} .upsell_medium_payment_item img {
      display: block;
      width: 100%;
      height: 100%;
    }
    /* =================================== FAQs ================= */
    {{ _section }} .faq_container {
      margin-block: 40px;
    }
    {{ _section }} .faq_title {
      color: #0C2016;
      font-family: "Lato";
      font-size: 26px;
      font-style: normal;
      font-weight: 500;
      line-height: 1.2;
      margin-bottom: 21px;
    }
    {{ _section }} .faq_items {
      margin-bottom: 21px;
    }
    {{ _section }} .faq_item {
      border-bottom: 1px solid #E8E8E8;
      margin-bottom: 17px;
    }
    {{ _section }} .faq_item.faq_hidden {
      display: none;
    }
    {{ _section }} .faq_item:last-child {
      margin-bottom: 0;
    }
    {{ _section }} .faq_question {
      margin-bottom: 9px;
      display: flex;
      justify-content: space-between;
      align-items: center;
    }
    {{ _section }} .faq_question p {
      margin: 0;
      color: #0C2016;
      font-family: "Lato";
      font-size: 14px;
      font-style: normal;
      font-weight: 400;
      line-height: 1.2;
      cursor: pointer;
    }
    {{ _section }} .faq_button {
      background-color: #E8E8E8;
      width: 24px;
      height: 24px;
      border-radius: 24px;
      display: flex;
      justify-content: center;
      align-items: center;
    }
