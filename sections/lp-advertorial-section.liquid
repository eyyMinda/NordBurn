                        {% assign benefits = bs.benefits | split: '@@@' %}
                        {% for benefit in benefits %}
                          <p class="upsell_medium_benefit">{{ benefit }}</p>
                        {% endfor %}
                      </div>
                    {% endif %}
                    <div class="upsell_medium_quantity">
                      <div class="upsell_medium_quantity_progress">
                        <div class="progress_container"></div>
                      </div>
                      <div class="upsell_medium_text">
                        Urgent! Only <span>12</span> units left in stock!
                      </div>
                    </div>
                    {% if bs.note != blank %}
                      <p class="upsell_medium_note">{{ bs.note }}</p>
                    {% endif %}
                    <a class="adv_btn" href="{{ _ss.upsell_medium_url }}">
                      <div class="upsell_medium_atc_btn" data-id="{{ bs.product.selected_or_first_available_variant.id }}" >{{ bs.button_text }}</div>
                    </a>
                    <div class="upsell_medium_atc_texts">
                      {% assign atc_usp_texts = bs.atc_usp_texts | split: '@@@' %}
                      {% for atc_usp_text in atc_usp_texts %}
                        <div class="upsell_medium_atc_text">{{ atc_usp_text }}</div>
                      {% endfor %}
                    </div>
                    <div class="upsell_medium_payment_container">
                      {%- for type in shop.enabled_payment_types -%}
                        <div class="upsell_medium_payment_item">
                          <img src="{{ type | payment_type_img_url }}" alt="payment type" width="auto" height="auto">
                        </div>
                      {%- endfor -%}
                    </div>
                  </div>
                </div>
              </div>
            </div>
          {% when 'faq-block' %}
            <div class="faq_container">
                <div class="faq_title">{{ bs.title }}</div>
                <div class="faq_items">
                    {% assign block_count = 0 %}
                    {% for block in section.blocks %}
                      {% if block.type == 'faq' %}
                        {% assign fs = block.settings %}
                        {% assign block_count = block_count | plus: 1 %}
                        <div class="faq_item {% unless block_count < 7 %}faq_hidden{% endunless %}">
                            <div class="faq_question">
                                <p>{{ fs.question }}</p>
                                <div class="faq_button">
                                    {% render 'icon-plus' %}
                                    {% render 'icon-minus' %}
                                </div>
                            </div>
                            <div class="faq_answer">{{ fs.answer }}</div>
                        </div>
                      {% endif %}
                    {% endfor %}
                </div>
                {% unless block_count < 7 %}
                    <div class="faq_see_more">See more</div>
                {% endunless %}
            </div>
          
        {% endcase %}
