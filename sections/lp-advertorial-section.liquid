                    {% if bs.trustpilot_image %}
                      <img src="{{ bs.trustpilot_image | img_url: '800x' }}" alt="trustpilot" class="trustpilot_image" width="auto" height="auto">
                    {% endif %}
                    <div class="benefit_text">{{ bs.benefit_text }}</div>
                    <style>
                      {{ _section }} .upsell_medium_quantity_progress .progress_container {
                        width: {{ 12 | times: 100 | divided_by: 15 }}%;
                      }
                    </style>

                    {% if bs.benefits != blank %}
