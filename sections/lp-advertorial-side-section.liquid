{% assign _ss = section.settings %}
{% assign _section = section.id | split: '__' | last | slice: -4, 4 | prepend: '#lp_advertorial_' %}

{%- capture section_content -%}
  <style>
    {{ _section }} .trustpilot_review {
      margin: 30px auto 8px;
      padding: 5px 16px;
      width: 100%;
      text-align: center;
    }

    {{ _section }} .trustpilot_review img {
      max-width: 500px;
      width: 100%;
    }

    {{ _section }} .title {
      margin: 0 14px 11px;
      color: var(--accent-color);
      text-align: center;
      font-family: "Lato";
      font-size: 28px;
      font-style: normal;
      font-weight: 700;
      line-height: 100%;
      letter-spacing: 0;
    }

    {{ _section }} .date_reading_time {
      margin: 0 14px 26px;
      color: #BFBFBF;
      text-align: center;
      font-family: "Lato";
      font-size: 16px;
      font-style: normal;
      font-weight: 400;
      line-height: 11px;
      letter-spacing: 0;
    }

    {{ _section }} .image_container {
      margin: 26px 14px;
      width: calc(100% - 28px);
      border-radius: 5px;
      overflow: hidden;
      transform: translateZ(0);
    }

    {{ _section }} .image_container img {
      width: 100%;
    }

    {{ _section }} .text_container p {
      margin: 21px 14px;
      color: black;
      font-family: "Lato";
      font-size: 15px;
      font-style: normal;
      font-weight: 400;
      line-height: 135%;
      letter-spacing: 0;
    }

    {{ _section }} .text_container p strong {
      color: #05906C;
      font-weight: 700;
    }

    {{ _section }} .quote_container {
      margin: 21px 14px;
      border-radius: 5px;
      background: var(--accent-color);
      padding: 15px;
      display: flex;
      box-shadow: 0px 4px 4px 0px #00000040;
    }

    {{ _section }} .quote_container::before {
      content: ' ';
      display: block;
      margin-right: 12px;
      width: 4px;
      min-width: 4px;
      height: auto;
      border-radius: 99px;
      background: #d2d9ff;
    }

    {{ _section }} .quote_holder p {
      margin: 0;
      color: #FFF;
      font-family: "Lato";
      font-size: 19px;
      font-style: italic;
      font-weight: 500;
      line-height: 115%;
      letter-spacing: 0;
    }

    {{ _section }} .quote_holder p strong {
      color: #d2d9ff;
    }

    {{ _section }} .warning_container {
      margin: 24px 14px;
      border-radius: 5px;
      background: var(--accent-color);
      padding: 18px 17px;
      display: flex;
    }

    {{ _section }} .warning_container::before {
      content: ' ';
      display: block;
      margin-right: 12px;
      width: 4px;
      min-width: 4px;
      height: auto;
      border-radius: 99px;
      background: #FE4643;
    }

    {{ _section }} .warning_title {
      color: #FE4643;
      font-family: "Lato";
      font-size: 19px;
      font-style: normal;
      font-weight: 700;
      line-height: 17px;
      margin-bottom: 8px;
    }

    {{ _section }} .warning_text p {
      margin: 0 0 16px 0;
      color: #FFF;
      font-family: "Lato";
      font-size: 13px;
      font-style: normal;
      font-weight: 400;
      line-height: 130%;
      letter-spacing: 0;
    }

    {{ _section }} .warning_text p:last-child {
      margin-bottom: 0;
    }

    {{ _section }} .half_decrease_container {
      margin: 26px 12px 26px 15px;
      display: flex;
      align-items: center;
    }

    {{ _section }} .half_decrease_container .mobile_image {
      display: block;
      margin-right: 14px;
      width: 87px;
      min-width: 87px;
      height: 61px;
    }

    {{ _section }} .half_decrease_container .desktop_image {
      display: none;
    }

    {{ _section }} .half_decrease_container p {
      margin: 0;
      color: var(--accent-color);
      font-family: "Lato";
      font-size: 24px;
      font-style: normal;
      font-weight: 700;
      line-height: 100%;
      letter-spacing: 0;
    }

    {{ _section }} .subtitle_container {
      margin: 35px 14px 0;
    }

    {{ _section }} .subtitle_container p {
      margin: 0;
      color: var(--accent-color);
      font-family: "Lato";
      font-size: 24px;
      font-style: normal;
      font-weight: 700;
      line-height: 100%;
      letter-spacing: 0;
    }

    {{ _section }} .subtitle_container p strong {
      color: #05906C;
    }

    {{ _section }} .text_over_image_container {
      margin: 26px 14px;
      height: 350px;
      position: relative;
      border-radius: 5px;
      overflow: hidden;
      transform: translateZ(0);
    }

    {{ _section }} .text_over_image_container img {
      display: block;
      width: 100%;
      height: 100%;
      object-fit: cover;
    }

    {{ _section }} .text_over_image_container .text_container {
      position: absolute;
      top: 0;
      left: 0;
      width: calc(100% - 40px);
      margin: 32px 20px;
    }

    {{ _section }} .text_over_image_container .main_text {
      margin-bottom: 16px;
    }

    {{ _section }} .text_over_image_container .main_text p {
      margin: 0;
      color: #37523A;
      text-align: center;
      font-family: "Lato";
      font-size: 24px;
      font-style: normal;
      font-weight: 700;
      line-height: 110%;
      letter-spacing: 0;
    }

    {{ _section }} .text_over_image_container .main_text p strong {
      color: #05906C;
    }

    {{ _section }} .text_over_image_container .normal_text {
      color: #37523A;
      text-align: center;
      font-family: "Lato";
      font-size: 15px;
      font-style: normal;
      font-weight: 700;
      line-height: 11px;
      text-transform: uppercase;
      letter-spacing: 0;
    }

    {{ _section }} .simple_upsell_container {
      margin: 24px 14px 21px;
      padding: 15px;
      border-radius: 5px;
      background: #FBFBF7;
    }

    {{ _section }} .simple_upsell_container img {
      display: block;
      margin-bottom: 15px;
      width: 100%;  
    }

    {{ _section }} .simple_upsell_button {
      display: block;
      text-decoration: none;
      border-radius: 99px;
      background: var(--accent-color);
      text-align: center;
      padding-top: 18px;
      padding-bottom: 18px;
      color: #d2d9ff;
      text-align: center;
      font-family: "Lato";
      font-size: 19px;
      font-style: normal;
      font-weight: 700;
      line-height: 13px;
      letter-spacing: 0;
      cursor: pointer;
    }

    {{ _section }} .flex-blocks {
      border-radius: 10px;
      display: flex;
      flex-direction: column;
      align-items: center;
      gap: 24px;
      padding: 24px 16px;
      margin: 24px 14px;
    }
    {{ _section }} .flex-item {
      max-width: 400px;
      width: 100%;
      border-radius: 10px;
      display: flex;
      flex-direction: column;
      align-items: center;
      gap: 16px;
      font-family: "Lato";
      text-align: center;
      padding: 16px 8px;
    }
    {{ _section }} .flex-item > * {
      max-width: 250px;
      width: 100%;
    }
    {{ _section }} .flex-item__title {
      font-weight: 700;
      font-size: 22px;
      line-height: 28px;
      text-decoration-line: underline;
      text-decoration-style: solid;
      text-underline-position: from-font;
      text-decoration-skip-ink: none;
    }
    {{ _section }} .flex-item__text {
      font-weight: 400;
      font-size: 18px;
      line-height: 21.6px;
      text-underline-position: from-font;
      text-decoration-skip-ink: none;
    }
    
    {{ _section }} .usp_container {
      --bg-color: transparent;
      margin: 24px 14px;
    }

    {{ _section }} .usp_title {
      margin-bottom: 24px;
      color: var(--accent-color);
      font-family: "Lato";
      font-size: 24px;
      font-style: normal;
      font-weight: 700;
      line-height: 17px;
      letter-spacing: 0;
    }

    {{ _section }} .usp_container .usp_item {
      margin-bottom: 21px;
      padding-bottom: 18px;
      border-bottom: 1px solid #D1D5DB;
    }

    {{ _section }} .usp_container .heading_container {
      display: flex;
      align-items: center;
      margin-bottom: 12px;
    }

    {{ _section }} .usp_container .heading_container img {
      display: block;
      width: 35px;
      min-width: 35px;
      height: 35px;
      margin-right: 13px;
    }
    {{ _section }} .usp_container .heading_container .usp_item_inline_heading {
      display: flex;
      flex-direction: column;
      gap: 12px;
    }

    {{ _section }} .usp_container .heading_container p {
      color: #05906C;
      font-family: "Poppins";
      font-size: 18px;
      font-style: normal;
      font-weight: 700;
      line-height: 13px;
      letter-spacing: 0;
      margin-block: 0;
    }

    {{ _section }} .usp_container .normal_container {
      color: var(--accent-color);
      font-family: "Lato";
      font-size: 15px;
      font-style: normal;
      font-weight: 400;
      line-height: 135%;
      letter-spacing: 0;
    }

    {{ _section }} .upsell_usp_container {
      margin: 40px 14px;
    }

    {{ _section }} .upsell_usp_subtitle {
      color: var(--accent-color);
      text-align: center;
      font-family: "Lato";
      font-size: 21px;
      font-style: normal;
      font-weight: 700;
      line-height: 15px;
      margin-bottom: 12px;
      letter-spacing: 0;
    }

    {{ _section }} .upsell_usp_title {
      color: #05906C;
      text-align: center;
      font-family: "Lato";
      font-size: 26px;
      font-style: normal;
      font-weight: 700;
      line-height: 22px;
      letter-spacing: 0;
      margin-bottom: 21px;
      object-fit: cover;
    }

    {{ _section }} .upsell_usp_image {
      display: block;
      width: 100%;
      border-radius: 5px;
      height: 350px;
      overflow: hidden;
      object-fit: cover;
      margin-bottom: 20px;
      transform: translateZ(0);
    }

    {{ _section }} .upsell_usp_items {
      display: grid;
      grid-template-columns: repeat(2, 1fr);
      gap: 15px 6px;
      margin-bottom: 21px;
    }

    {{ _section }} .upsell_usp_item {
      display: flex;
      align-items: center;
    }

    {{ _section }} .upsell_usp_item svg {
      display: block;
      width: 21px;
      min-width: 21px;
      height: 21px;
      margin-right: 7px;
    }

    {{ _section }} .upsell_usp_item svg path {
      fill: #37523A;
    }

    {{ _section }} .upsell_usp_item p {
      margin: 0;
      color: #000;
      font-family: "Lato";
      font-size: 14px;
      font-style: normal;
      font-weight: 500;
      line-height: 15px;
      letter-spacing: 0;
    }

    {{ _section }} .upsell_usp_quantity {
      margin: 0 14px 15px;
    }

    {{ _section }} .upsell_usp_quantity_progress {
      margin-bottom: 8px;
      position: relative;
      width: 100%;
      height: 6px;
      border-radius: 99px;
      border: 1px solid var(--accent-color);
      background: rgba(47, 19, 19, 0.00);
    }

    {{ _section }} .upsell_usp_quantity_progress .progress_container {
      display: block;
      border-radius: 99px;
      position: absolute;
      height: 6px;
      background: #FE4643;
      top: -1px;
      left: -1px;
    }

    {{ _section }} .upsell_usp_text {
      color: #000;
      text-align: center;
      font-family: "Lato";
      font-size: 14px;
      font-style: normal;
      font-weight: 500;
      line-height: 10px;
      letter-spacing: 0;
    }

    {{ _section }} .upsell_usp_text span {
      color: #FE4643;
      font-weight: 700;
    }

    {{ _section }} .upsell_usp_atc_btn_wrapper {
      margin-bottom: 8px;
      width: 100%;
      height: 55px;
      border-radius: 30px;
      background: #23D160;
      position: relative;
      overflow: hidden;
      padding: 5px;
      transform: translateZ(0);
    }

    {{ _section }} .upsell_usp_atc_btn_wrapper::before {
      content: '';
      display: block;
      background: conic-gradient(#23D160, #23D160, #23D160, #23D160, #FAF6F0, #fff);
      width: 110%;
      padding-bottom: 216%;
      position: absolute;
      left: 50%;
      top: 50%;
      transform: translate(-50%, -50%);
      border-radius: 100%;
      z-index: 2;
      animation: button_border_spin 2.5s linear infinite;
    }

    {{ _section }} .upsell_usp_atc_btn {
      position: absolute;
      width: calc(100% - 10px);
      height: 45px;
      border-radius: 30px;
      background: #23D160;
      display: flex;
      align-items: center;
      justify-content: center;
      cursor: pointer;
      z-index: 3;
      color: #FFF;
      text-align: center;
      font-family: "Lato";
      font-size: 23px;
      font-style: normal;
      font-weight: 700;
      line-height: 18px;
      letter-spacing: 0;
    }

    {{ _section }} .upsell_usp_atc_texts {
      display: flex;
      margin: 0 auto 15px;
      width: fit-content;
      height: 18px;
    }

    {{ _section }} .upsell_usp_atc_text {
      display: block;
      color: var(--accent-color);
      font-family: "Lato";
      font-size: 13px;
      font-style: normal;
      font-weight: 500;
      line-height: 18px;
      letter-spacing: 0;
      padding: 0 12px;
      border-right: 1px solid var(--accent-color);
    }

    {{ _section }} .upsell_usp_atc_text:first-child {
      padding-left: 0;
    }

    {{ _section }} .upsell_usp_atc_text:last-child {
      padding-right: 0;
      border-right: none;
    }

    {{ _section }} .upsell_usp_payment_container {
      display: flex;
      margin: 0 auto;
      width: fit-content;
      height: 24px;
    }

    {{ _section }} .upsell_usp_payment_item {
      display: block;
      width: 36px;
      height: 24px;
      border-radius: 2px;
      margin-right: 8px;
    }

    {{ _section }} .upsell_usp_payment_item:last-child {
      margin-right: 0;
    }

    {{ _section }} .upsell_usp_payment_item img {
      display: block;
      width: 100%;
      height: 100%;
    }

    {{ _section }} .review_container {
      padding: 38px 14px;
      background: #FBFBF7;
    }

    {{ _section }} .review_title {
      margin: 0 auto 12px;
      padding: 0 55px;
      color: var(--accent-color);
      text-align: center;
      font-family: "Lato";
      font-size: 24px;
      font-style: normal;
      font-weight: 700;
      line-height: 20px;
      letter-spacing: 0;
    }

    {{ _section }} .upsell_medium_container {
      padding: 40px 14px;
    }

    {{ _section }} .upsell_medium_subtitle {
      color: var(--accent-color);
      font-family: "Lato";
      font-size: 21px;
      font-style: normal;
      font-weight: 700;
      line-height: 24px;
      letter-spacing: 0;
      margin-bottom: 12px;
    }

    {{ _section }} .upsell_medium_title {
      color: #05906C;
      font-family: "Lato";
      font-size: 26px;
      font-style: normal;
      font-weight: 700;
      line-height: 24px;
      letter-spacing: 0;
      margin-bottom: 21px;
    }
    {{ _section }} .upsell_medium_description {
      color: black;
      font-family: "Lato";
      font-style: normal;
      font-weight: 400;
      font-size: 18px;
      line-height: 24px;
      text-align: center;
      margin-bottom: 12px;
    }

    {{ _section }} .upsell_medium_content {
      border-radius: 5px;
      background: #0C2016;
      padding: 21px 15px;
    }

    {{ _section }} .upsell_medium_content .desktop_container {
      display: none;
    }

    {{ _section }} .upsell_medium_image {
      display: block;
      width: 100%;
      margin-bottom: 16px;
    }

    {{ _section }} .upsell_medium_quantity {
      margin-bottom: 8px;
    }

    {{ _section }} .upsell_medium_quantity_progress {
      display: block;
      height: 6px;
      position: relative;
      border-radius: 99px;
      border: 1px solid #FFF;
      background: rgba(47, 19, 19, 0.00);
      margin-bottom: 8px;
    }

    {{ _section }} .upsell_medium_quantity_progress .progress_container {
      display: block;
      border-radius: 99px;
      position: absolute;
      height: 6px;
      background: #FE4643;
      top: -1px;
      left: -1px;
    }

    {{ _section }} .upsell_medium_quantity .upsell_medium_text {
      color: #FFF;
      text-align: center;
      font-family: "Lato";
      font-size: 14px;
      font-style: normal;
      font-weight: 500;
      line-height: 10px;
      letter-spacing: 0;
    }

    {{ _section }} .upsell_medium_quantity .upsell_medium_text span {
      color: #FE4643;
      font-weight: 700;
    }

    {{ _section }} .upsell_medium_atc_btn {
      display: block;
      width: 100%;
      max-width: 500px;
      background: #05906C;
      border-radius: 99px;
      padding-top: 18px;
      padding-bottom: 18px;
      text-decoration: none;
      color: #FFF;
      text-align: center;
      font-family: "Lato";
      font-size: 19px;
      font-style: normal;
      font-weight: 700;
      line-height: 13px;
      letter-spacing: 0;
      cursor: pointer;
      margin: 0 auto 8px;
    }

    {{ _section }} .upsell_medium_atc_texts {
      display: flex;
      margin: 0 auto 15px;
      width: fit-content;
      height: 18px;
    }

    {{ _section }} .upsell_medium_atc_text {
      display: block;
      color: #fff;
      font-family: "Lato";
      font-size: 13px;
      font-style: normal;
      font-weight: 500;
      line-height: 18px;
      letter-spacing: 0;
      padding: 0 12px;
      border-right: 1px solid #fff;
    }

    {{ _section }} .upsell_medium_atc_text:first-child {
      padding-left: 0;
    }

    {{ _section }} .upsell_medium_atc_text:last-child {
      padding-right: 0;
      border-right: none;
    }

    {{ _section }} .upsell_medium_payment_container {
      display: flex;
      margin: 0 auto;
      width: fit-content;
      height: 24px;
    }

    {{ _section }} .upsell_medium_payment_item {
      display: block;
      width: 36px;
      height: 24px;
      border-radius: 2px;
      margin-right: 8px;
    }

    {{ _section }} .upsell_medium_payment_item:last-child {
      margin-right: 0;
    }

    {{ _section }} .upsell_medium_payment_item img {
      display: block;
      width: 100%;
      height: 100%;
    }

    {{ _section }} .faq_container {
      margin: 40px 14px;
    }

    {{ _section }} .faq_title {
      color: #0C2016;
      font-family: "Lato";
      font-size: 26px;
      font-style: normal;
      font-weight: 500;
      line-height: 18px;
      letter-spacing: 0;
      margin-bottom: 21px;
    }

    {{ _section }} .faq_items {
      margin-bottom: 21px;
    }

    {{ _section }} .faq_item {
      border-bottom: 1px solid #E8E8E8;
      margin-bottom: 17px;
    }

    {{ _section }} .faq_item.faq_hidden {
      display: none;
    }

    {{ _section }} .faq_item:last-child {
      margin-bottom: 0;
    }

    {{ _section }} .faq_question {
      margin-bottom: 9px;
      display: flex;
      justify-content: space-between;
      align-items: center;
    }

    {{ _section }} .faq_question p {
      margin: 0;
      color: #0C2016;
      font-family: "Lato";
      font-size: 14px;
      font-style: normal;
      font-weight: 400;
      line-height: 14px;
      letter-spacing: 0;
      cursor: pointer;
    }

    {{ _section }} .faq_button {
      background-color: #E8E8E8;
      width: 24px;
      height: 24px;
      border-radius: 24px;
      display: flex;
      justify-content: center;
      align-items: center;
    }

    {{ _section }} .faq_button svg {
      display: block;
      width: 12px;
      height: 12px;
    }

    {{ _section }} .faq_button svg:first-child {
      display: block;
    }

    {{ _section }} .faq_button svg:last-child {
      display: none;
    }

    {{ _section }} .faq_item.active .faq_button svg:first-child {
      display: none;
    }

    {{ _section }} .faq_item.active .faq_button svg:last-child {
      display: block;
    }

    {{ _section }} .faq_answer {
      display: none;
      margin: 0;
      color: #0C2016;
      font-family: "Lato";
      font-size: 14px;
      font-style: normal;
      font-weight: 400;
      line-height: 14px;
      letter-spacing: 0;
      margin-bottom: 9px;
    }

    {{ _section }} .faq_see_more {
      cursor: pointer;
      text-decoration: underline;
      color: #000;
      font-family: "Lato";
      font-size: 14px;
      font-style: normal;
      font-weight: 700;
      line-height: 14px;
      letter-spacing: 0;
    }

    {{ _section }} .right_container {
      display: none;
    }

    {{ _section }} .right_container .shopify-section .page-width {
      padding: 0;
    }

    #lp_advertorial_sticky_button {
      position: fixed;
      bottom: 0;
      left: 0;
      right: 0;
      display: flex;
      padding: 11px 5px 10px 5px;
      justify-content: center;
      align-items: center;
      background: #FFF;
      box-shadow: 0px -15px 24px 0px rgba(1, 41, 0, 0.50);
      z-index: 5;
    }

    #lp_advertorial_sticky_button .button_container {
      --btn-accent-color: var(--accent-color);
      --btn-foreground-color: #d2d9ff;
      width: 100%;
      padding-top: 18px;
      padding-bottom: 18px;
      border: 2px solid var(--btn-accent-color);
      background: var(--btn-accent-color);
      color: var(--btn-foreground-color);
      text-align: center;
      font-family: "Lato";
      font-size: 19px;
      font-style: normal;
      font-weight: 700;
      line-height: 13px;
      border-radius: 99px;
      letter-spacing: 0;
      cursor: pointer;
      transition: all 150ms ease-in-out;
    }
    #lp_advertorial_sticky_button .button_container:hover {
      background: var(--btn-foreground-color);
      color: var(--btn-accent-color);
      opacity: 1;
    }

    @media screen and (min-width: 800px) {
