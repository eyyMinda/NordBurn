  <div id="lp_advertorial_{{ section.id | split: '__' | last | slice: -4, 4 }}">
    <div class="product_section_holder">
      <div class="left_container">
        {% for block in section.blocks %}
          {% assign bs = block.settings %}
          {% case block.type %}
            {% when 'trustpilot-review' %}
              <div class="trustpilot_review">
                <img src="{{ bs.image | img_url: '1000x' }}" alt="review">
              </div>
            {% when 'title' %}
              <div class="title">{{ bs.title }}</div>
            {% when 'date-reading-time' %}
              <div class="date_reading_time">{{ bs.text }}</div>
            {% when 'image' %}
              <div class="image_container">
                <img src="{{ bs.image | img_url: '1000x' }}" alt="{{ bs.image.alt }}">
              </div>
            {% when 'text' %}
              <div class="text_container">{{ bs.text }}</div>
            {% when 'quote' %}
              <div class="quote_container">
                <div class="quote_holder">{{ bs.text }}</div>  
              </div>
            {% when 'warning-block' %}
              <div class="warning_container">
                <div class="warning_holder">
                  <div class="warning_title">{{ bs.title }}</div>
                  <div class="warning_text">{{ bs.text }}</div>
                </div>  
              </div>
            {% when 'half-decrease' %}
              <div class="half_decrease_container">
                <img class="mobile_image" src="{{ bs.image_m |  img_url: '500x' }}" alt="mobile image">
                <img class="desktop_image" src="{{ bs.image_d | img_url: '700x' }}" alt="desktop image">
                <p>{{ bs.main_text }}</p>
              </div>
            {% when 'subtitle' %}
              <div class="subtitle_container">{{ bs.text }}</div>
            {% when 'text_over_image' %}
              <div class="text_over_image_container">
                <img src="{{ bs.image | img_url: '1000x' }}" alt="{{ bs.image.alt }}">
                <div class="text_container">
                  <div class="main_text">{{ bs.main_text }}</div>
                  <div class="normal_text">{{ bs.text }}</div>
                </div>
              </div>
            {% when 'simple-upsell' %}
              <div class="simple_upsell_container">
                <img src="{{ bs.image | img_url: '1000x' }}" alt="Simple Upsell Image">
                <div onclick="slideToUpsellUspSection()" class="simple_upsell_button">{{ bs.button_text }}</div>
              </div>
            {% when 'usp' %}
              <div class="usp_container">
                <div class="usp_title">{{ bs.title }}</div>
                {% assign usp_items = section.blocks | where: 'type', 'usp-item' %}
                {% for usp_item_block in usp_items %}
                    <div class="usp_item">
                      <div class="heading_container">
                        <img src="{{ usp_item_block.settings.image | img_url: '1000x' }}" alt="{{ usp_item_bs.heading_text }}">
                        {% if bs.text_inline %}
                          <div class="usp_item_inline_heading">
                            <p>{{ usp_item_block.settings.heading_text }}</p>
                            <div class="normal_container">{{ usp_item_block.settings.normal_text }}</div>
                          </div>
                        {% else %}
                          <p>{{ usp_item_block.settings.heading_text }}</p>
                        {% endif %}
                      </div>
                      {% unless bs.text_inline %}
                        <div class="normal_container">{{ usp_item_block.settings.normal_text }}</div>
                      {% endunless %}
                    </div>
                {% endfor %}
              </div>
            {% when 'flex-blocks' %}
              <div class="flex-blocks" style="background-color: {{ bs.bg_color }};">
                {% assign flex_items = section.blocks | where: 'type', 'flex-item' %}
                {% for item in flex_items %}
                  {% assign is = item.settings %}
                  <div class="flex-item" style="background-color: {{ bs.item_bg_color }}; color: {{ bs.item_text_color }};">
                    {% if is.title != blank %}
                      <div class="flex-item__title">{{ is.title }}</div>
                    {% endif %}
                    {% if is.text != blank %}
                      <div class="flex-item__text">{{ is.text }}</div>
                    {% endif %}
                  </div>
                {% endfor %}
              </div>
            {% when 'upsell-usp' %}
              <div class="upsell_usp_container" id="upsell_usp">
                <div class="upsell_usp_subtitle">{{ bs.subtitle }}</div>
                <div class="upsell_usp_title">{{ bs.title }}</div>
                <img src="{{ bs.image | img_url: '1000x' }}" alt="{{ bs.title }}" class="upsell_usp_image">
                <div class="upsell_usp_items">
                  {% assign usp_item_texts = bs.usp_item_texts | split: '@@@' %}
                  {% for usp_item_text in usp_item_texts %}
                    <div class="upsell_usp_item">
                      {% render 'icon-circle-check' %}
                      <p>{{ usp_item_text }}</p>
                    </div>
                  {% endfor %}
                </div>
                {% if bs.product.selected_or_first_available_variant.inventory_quantity < bs.stock_notification_limit %}
                  <style>
                    {{ _section }} .upsell_usp_quantity_progress .progress_container {
                      width: {{ 12 | times: 100 | divided_by: 15 }}%;
                    }
                  </style>
                  <div class="upsell_usp_quantity">
                    <div class="upsell_usp_quantity_progress">
                      <div class="progress_container"></div>
                    </div>
                    <div class="upsell_usp_text">
                      Urgent! Only <span>12</span> units left in stock!
                    </div>
                  </div>
                {% endif %}
                <div class="upsell_usp_atc_btn_wrapper">

                  {% if template contains 'advertorial-lp-1' %}
                    <a href="{% if request.path contains '/pe-adv' %}/pages/advertorial-funnel-subscribe{% elsif template contains 'advertorial-side-lp' %}/pages/offer-funnel{% else %}/pages/advertorial-funnel{% endif %}" class="right_upsell_usp_atc" data-id="{{ bs.product.selected_or_first_available_variant.id }}">{{ bs.button_text }}</a>
                  {% else %}
                    <div class="upsell_usp_atc_btn" data-id="{{ bs.product.selected_or_first_available_variant.id }}">{{ bs.button_text }}</div>
                  {% endif %}
                  
                </div>
                <div class="upsell_usp_atc_texts">
                  {% assign atc_usp_texts = bs.atc_usp_texts | split: '@@@' %}
                  {% for atc_usp_text in atc_usp_texts %}
                    <div class="upsell_usp_atc_text">{{ atc_usp_text }}</div>
                  {% endfor %}
                </div>
