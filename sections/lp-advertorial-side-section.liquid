                        <div class="upsell_medium_atc_btn" data-id="{{ bs.product.selected_or_first_available_variant.id }}" >{{ bs.button_text }}</div>
                      </a>
                    {% else %}
                      <div class="upsell_medium_atc_btn" data-id="{{ bs.product.selected_or_first_available_variant.id }}" onclick="slideToUpsellUspSection()">{{ bs.button_text }}</div>
                    {% endif %}
                    <div class="upsell_medium_atc_texts">
